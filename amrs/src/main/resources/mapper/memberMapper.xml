<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.application.amrs.member.dao.MemberDAO">

<insert id="insertMember" parameterType="MemberDTO">
	INSERT INTO MEMBER
	VALUES (
		#{memberId},
		#{email},
		#{passwd},
		#{birthAt},
		#{memberPhoneNumber},
		#{memberProfileOriginalName},
		#{memberProfileUUID},
		#{smsstsYn},
		#{emailstsYn},
		#{zipcode},
		#{roadAddress},
		#{jibunAddress},
		#{namujiAddress},
		#{memberPoint},
		#{memberRole},
		#{createDt},
		#{upadateDt}
	)
</insert>
</mapper>